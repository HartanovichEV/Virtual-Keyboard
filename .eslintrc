{
    "rules": {
        "semi": ["error", "always"],
        "quotes": ["error", "double"]
    },

    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },

    "env": {
        "es2021": true,
        "browser": true
    }
}